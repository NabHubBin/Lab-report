#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main(){
    string str = "Hello World";
    reverse(str.begin(), str.end());
    cout << "Reversed string: " << str << endl;
    string str2 = "Hello World";
    int n = str2.length();
    for (int i = 0; i < n / 2; i++){
    swap(str2[i], str2[n - i - 1]);
}
    cout << "Reversed string (manual swap): " << str2 << endl;

    return 0;
}
