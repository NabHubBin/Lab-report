#include <iostream>
using namespace std;

int main(){
    int rows, cols;

    cout << "Enter number of Rows: ";
    cin >> rows;

    cout << "Enter number of Columns: ";
    cin >> cols;

    int arr[100][100];

    cout << "Enter Elements of the Array:" << endl;
    for (int i = 0; i < rows; i++){
    for (int j = 0; j < cols; j++){
    cin >> arr[i][j];
}
}

    cout << "The 2D array is:" << endl;
    for (int i = 0; i < rows; i++){
        for (int j = 0; j < cols; j++){
    cout << arr[i][j] << " ";
}
    cout << endl;
}

    return 0;
}
