#include <iostream>
using namespace std;

int main(){
    int arr1[] = {10, 20, 30, 40, 50};
    int n = sizeof(arr1) / sizeof(arr1[0]);

    int arr2[n];

    for (int i = 0; i < n; i++){
        arr2[i] = arr1[i];
}

    cout << "Elements of the Original array:";
    for (int i = 0; i < n; i++){
    cout << arr1[i] << " ";
}

    cout << "\nElements of the Copied array:";
    for (int i = 0; i < n; i++){
    cout << arr2[i] << " ";
}

    return 0;
}
