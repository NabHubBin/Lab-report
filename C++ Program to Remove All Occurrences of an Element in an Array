#include <iostream>
using namespace std;

int removeAllOccurrences(int arr[], int n, int key){
    int j = 0;

    for (int i = 0; i < n; i++){
    if (arr[i] != key) {
        arr[j++] = arr[i];
}
}
    return j;
}

int main(){
    int arr[] = {3, 5, 2, 3, 8, 3, 9};
    int n = sizeof(arr) / sizeof(arr[0]);
    int key = 3;

    int newSize = removeAllOccurrences(arr, n, key);

    cout << "Array after removing all occurrences of " << key << ": ";
    for (int i = 0; i < newSize; i++){
    cout << arr[i] << " ";
}
    return 0;
}
