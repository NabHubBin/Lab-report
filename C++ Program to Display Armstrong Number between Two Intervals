#include <iostream>
#include <cmath>
using namespace std;

bool isArmstrong(int num) {
    int originalNum = num;
    int n = 0;
    double sum = 0.0;
    while (originalNum != 0) {
        originalNum /= 10;
        n++;
}

originalNum = num;

    while (originalNum != 0) {
        int digit = originalNum % 10;
        sum += pow(digit, n);
        originalNum /= 10;
}

    return (int)sum == num;
}

int main() {
    int start, end;

    cout << "Enter starting interval: ";
    cin >> start;
    cout << "Enter ending interval: ";
    cin >> end;

    cout << "Armstrong numbers between " << start << " and " << end << " are:\n";

    for (int i = start; i <= end; i++) {
        if (isArmstrong(i)) {
            cout << i << " ";
}
}

 return 0;
}
