#include <iostream>
using namespace std;

bool isPrime(int n){
    if (n <= 1)
    return false;

    for (int i = 2; i * i <= n; i++){
        if (n % i == 0)
    return false;
}
    return true;
}

void displayPrimes(int low, int high){
    cout << "Prime numbers between " << low << " and " << high << " are: ";

    for (int i = low; i <= high; i++){
        if (isPrime(i))
    cout << i << " ";
}
}

int main(){
    int low, high;

    cout << "Enter the lower interval: ";
    cin >> low;

    cout << "Enter the upper interval: ";
    cin >> high;

    displayPrimes(low, high);

    return 0;
}
