#include <iostream>
#include <string>
using namespace std;

int main(){
    string str = "Hello";
    int index = 1;
    if (index >= 0 && index < str.length()){
    char ch = str[index];
    cout << "Character at index " << index << " : " << ch << endl;
    cout << "Unicode (ASCII) code point: " << int(ch) << endl;
} else {
    cout << "Index out of Bounds!" << endl;
}

    return 0;
}
