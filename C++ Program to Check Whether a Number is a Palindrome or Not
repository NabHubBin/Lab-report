#include <iostream>
using namespace std;

int main() {
    int num, originalNum, reversed = 0;
    cout << "Enter a number: ";
    cin >> num;

    originalNum = num;
    while (num != 0) {
        int digit = num % 10;
        reversed = reversed * 10 + digit;
        num /= 10;
}

    if (originalNum == reversed)
        cout << originalNum << " is a palindrome.";
    else
        cout << originalNum << " is not a palindrome.";

    return 0;
}
