#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int rows = 3, cols = 3;

    int matrix[3][3] ={
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
};

    int trace = 0;
    double norm = 0.0;

    for (int i = 0; i < rows; i++){
    for (int j = 0; j < cols; j++){
        if (i == j)
        trace += matrix[i][j];
        norm += matrix[i][j] * matrix[i][j];
}
}

    norm = sqrt(norm);

    cout << "Trace of the matrix = " << trace << endl;
    cout << "Frobenius norm of the matrix = " << norm << endl;

    return 0;
}
