#include <iostream>
#include <cmath>
using namespace std;

bool isPrime(int num){
    if (num <= 1)
        return false;
    for (int i = 2; i <= sqrt(num); i++){
    if (num % i == 0)
    return false;
}
return true;
}
int main(){
    int start, end;
    cout << "Enter Starting interval:";
    cin >> start;
    cout << "Enter Ending interval: ";
    cin >> end;

    cout << "Prime numbers between " << start << " and " << end << " are:\n";

    for (int i = start; i <= end; i++) {
        if (isPrime(i))
    cout << i << " ";
}

return 0;
}
