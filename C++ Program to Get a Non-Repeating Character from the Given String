#include <iostream>
#include <unordered_map>
#include <string>
using namespace std;

int main(){
    string str = "swiss";

    unordered_map<char, int> freq;
    for (char ch : str){
        freq[ch]++;
}
    char nonRepeating = '\0';
    for (char ch : str){
    if (freq[ch] == 1){
        nonRepeating = ch;
    break;
}
}
    if (nonRepeating != '\0')
        cout << "First Non-repeating Character: " << nonRepeating << endl;
    else
        cout << "No Non-repeating Character Found." << endl;

    return 0;
}
