#include <iostream>
using namespace std;

int main(){
    int rows = 3, cols = 3;
    int mat1[3][3] ={
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
};

    int mat2[3][3] ={
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
};

    bool equal = true;
    for (int i = 0; i < rows; i++){
    for (int j = 0; j < cols; j++){
        if (mat1[i][j] != mat2[i][j]){
        equal = false;
    break;
}
}
        if (!equal)
    break;
}

    if (equal)
        cout << "The two matrices are equal." << endl;
    else
        cout << "The two matrices are NOT equal." << endl;

    return 0;
}
