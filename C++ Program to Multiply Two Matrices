#include <iostream>
using namespace std;

int main(){
    int rows1 = 2, cols1 = 3;
    int rows2 = 3, cols2 = 2;

    int mat1[2][3] ={
        {1, 2, 3},
        {4, 5, 6}
};
    int mat2[3][2] ={
        {7, 8},
        {9, 10},
        {11, 12}
};
    int result[2][2] = {0};
    if (cols1 != rows2){
        cout << "Matrix multiplication not Possible!" << endl;
        return 0;
}

    for (int i = 0; i < rows1; i++){
    for (int j = 0; j < cols2; j++){
    for (int k = 0; k < cols1; k++){
            result[i][j] += mat1[i][k] * mat2[k][j];
}
}
}
    cout << "Product of the two matrices:\n";
    for (int i = 0; i < rows1; i++){
    for (int j = 0; j < cols2; j++){
    cout << result[i][j] << " ";
}
    cout << endl;
}
    return 0;
}
